{
  "$schema": "https://beta.frontmatter.codes/frontmatter.schema.json",
  "frontMatter.experimental": true,
  "frontMatter.extensibility.scripts": [
    "[[workspace]]/.frontmatter/ui/external.js"
  ],
  "frontMatter.sponsors.ai.enabled": true,
  "frontMatter.git.enabled": true,
  "frontMatter.git.actions": true,
  "frontMatter.git.submodule.push": true,
  "frontMatter.git.submodule.pull": true,
  "frontMatter.git.submodule.branch": "main",
  "frontMatter.git.submodule.folder": ".frontmatter",
  "frontMatter.dashboard.content.pagination": true,
  "frontMatter.dashboard.contentTags": "tags",
  "frontMatter.content.autoUpdateDate": false,
  "frontMatter.content.fmHighlight": false,
  "frontMatter.content.wysiwyg": true,
  "frontMatter.content.pageFolders": [
    {
      "title": "Pages",
      "path": "[[workspace]]/.frontmatter/content",
      "excludeSubdir": true
    },
    {
      "title": "Blog posts",
      "path": "[[workspace]]/.frontmatter/content/posts"
    },
    {
      "title": "projects",
      "path": "[[workspace]]/.frontmatter/content/projects"
    }
  ],
  "frontMatter.content.sorting": [
    {
      "title": "Date (asc)",
      "name": "date",
      "order": "asc",
      "type": "date"
    },
    {
      "title": "Date (desc)",
      "name": "date",
      "order": "desc",
      "type": "date"
    }
  ],
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "pageBundle": false,
      "fields": [
        {
          "title": "Title",
          "name": "title",
          "type": "string",
          "required": true
        },
        {
          "title": "Title (long)",
          "name": "longTitle",
          "type": "string"
        },
        {
          "title": "Custom field",
          "name": "customField",
          "type": "customField",
          "customType": "customField"
        },
        {
          "title": "Slug",
          "name": "slug",
          "type": "slug",
          "required": true
        },
        {
          "title": "Description",
          "name": "description",
          "type": "string",
          "required": true
        },
        {
          "title": "Publishing date",
          "name": "date",
          "type": "datetime",
          "required": true
        },
        {
          "title": "Modified date",
          "name": "lastmod",
          "type": "datetime"
        },
        {
          "title": "Content preview",
          "name": "preview",
          "type": "image",
          "default": "{{ogImage}}",
          "required": true
        },
        {
          "title": "Is in draft",
          "name": "draft",
          "type": "draft",
          "default": false
        },
        {
          "title": "Comments",
          "name": "comments",
          "type": "boolean",
          "default": true
        },
        {
          "title": "Tags",
          "name": "tags",
          "type": "tags",
          "default": [],
          "required": true
        },
        {
          "title": "type",
          "name": "type",
          "type": "string",
          "default": "post",
          "hidden": true
        }
      ]
    }
  ],
  "frontMatter.content.placeholders": [
    {
      "id": "ogImage",
      "script": "./scripts/og-image.js",
      "command": "~/.nvm/versions/node/v16.19.0/bin/node"
    }
  ],
  "frontMatter.preview.host": "http://localhost:1313",
  "frontMatter.content.publicFolder": ".frontmatter/static",
  "frontMatter.content.defaultSorting": "DateDesc",
  "frontMatter.custom.scripts": [
    {
      "title": "Generate social image",
      "script": "./scripts/social-img.js",
      "command": "~/.nvm/versions/node/v16.19.0/bin/node"
    },
    {
      "title": "LQIP (macOS)",
      "script": "./scripts/lqip.js",
      "command": "~/.nvm/versions/node/v16.19.0/bin/node"
    },
    {
      "title": "Publish GitHub Stars profile",
      "script": "./scripts/github-stars.js",
      "command": "~/.nvm/versions/node/v16.19.0/bin/node"
    },
    {
      "title": "Optimize media",
      "script": "./scripts/media.js",
      "command": "~/.nvm/versions/node/v16.19.0/bin/node",
      "type": "mediaFile"
    },
    {
      "title": "Optimize images",
      "script": "./scripts/media-all.js",
      "command": "~/.nvm/versions/node/v16.19.0/bin/node",
      "type": "mediaFolder"
    }
  ],
  "frontMatter.dashboard.openOnStart": true,
  "frontMatter.taxonomy.slugPrefix": "/",
  "frontMatter.taxonomy.slugSuffix": "/",
  "frontMatter.framework.id": "hugo",
  "frontMatter.taxonomy.customTaxonomy": [
    {
      "id": "customTags",
      "options": [
        "dsafdfas"
      ]
    }
  ],
  "frontMatter.data.files": [
    {
      "id": "projects",
      "title": "Projects",
      "file": "[[workspace]]/themes/eliostruyf-v2/data/projects.json",
      "fileType": "json",
      "labelField": "title",
      "schema": {
        "title": "Sponsors",
        "type": "object",
        "required": [
          "title",
          "href"
        ],
        "properties": {
          "title": {
            "type": "string"
          },
          "href": {
            "type": "string"
          }
        }
      }
    }
  ],
  "frontMatter.data.folders": [
    {
      "id": "Testable",
      "path": "[[workspace]]/themes/eliostruyf-v2/data/",
      "labelField": "title",
      "schema": {
        "title": "Sponsors",
        "type": "object",
        "required": [
          "title",
          "href"
        ],
        "properties": {
          "title": {
            "type": "string"
          },
          "href": {
            "type": "string"
          }
        }
      }
    }
  ],
  "frontMatter.content.snippets": {
    "Image snippet": {
      "body": "{{< caption-new \"[[&mediaUrl]]\" \"[[caption]]\" >}}",
      "isMediaSnippet": true,
      "description": ""
    },
    "Video snippet": {
      "body": [
        "{{< video \"[[&mediaUrl]]\" \"[[caption]]\" >}}"
      ],
      "isMediaSnippet": true
    },
    "Highlight (single)": {
      "description": "Creates a code highlighting box",
      "body": [
        "{{< highlight [[type]] \"linenos=table,noclasses=false\" >}}",
        "[[&selection]]",
        "{{< / highlight >}}"
      ],
      "fields": [
        {
          "name": "type",
          "title": "Language",
          "type": "choice",
          "choices": [
            "html",
            "css",
            "typescript",
            "json",
            "bash"
          ],
          "default": "typescript"
        },
        {
          "name": "selection",
          "title": "Selection",
          "type": "string",
          "default": "FM_SELECTED_TEXT"
        }
      ]
    },
    "Blockquote": {
      "body": "{{< blockquote type=\"[[type]]\" text=\"[[&selection]]\" >}}",
      "description": "Creates a blockquote",
      "fields": [
        {
          "name": "type",
          "title": "Type",
          "type": "choice",
          "choices": [
            "info",
            "important"
          ],
          "default": "info"
        },
        {
          "name": "selection",
          "title": "Selection",
          "type": "string",
          "default": "FM_SELECTED_TEXT"
        }
      ]
    }
  }
}