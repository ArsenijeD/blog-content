  "frontMatter.data.files": [
    {
      "id": "projects",
      "title": "Projects",
      "file": "[[workspace]]/themes/eliostruyf-v2/data/projects.json",
      "fileType": "json",
      "labelField": "title",
      "schema": {
        "title": "Sponsors",
        "type": "object",
        "required": [
          "title",
          "href"
        ],
        "properties": {
          "title": {
            "type": "string"
          },
          "href": {
            "type": "string"
          }
        }
      }
    }
  ],
  "frontMatter.data.folders": [
    {
      "id": "Testable",
      "path": "[[workspace]]/themes/eliostruyf-v2/data/",
      "labelField": "title",
      "schema": {
        "title": "Sponsors",
        "type": "object",
        "required": [
          "title",
          "href"
        ],
        "properties": {
          "title": {
            "type": "string"
          },
          "href": {
            "type": "string"
          }
        }
      }
    }
  ],
  "frontMatter.content.snippets": {
    "Image snippet": {
      "body": "{{< caption-new \"[[&mediaUrl]]\" \"[[caption]]\" >}}",
      "isMediaSnippet": true,
      "description": ""
    },
    "Video snippet": {
      "body": [
        "{{< video \"[[&mediaUrl]]\" \"[[caption]]\" >}}"
      ],
      "isMediaSnippet": true
    },
    "Highlight (single)": {
      "description": "Creates a code highlighting box",
      "body": [
        "{{< highlight [[type]] \"linenos=table,noclasses=false\" >}}",
        "[[&selection]]",
        "{{< / highlight >}}"
      ],
      "fields": [
        {
          "name": "type",
          "title": "Language",
          "type": "choice",
          "choices": [
            "html",
            "css",
            "typescript",
            "json"
          ],
          "default": "typescript"
        },
        {
          "name": "selection",
          "title": "Selection",
          "type": "string",
          "default": "FM_SELECTED_TEXT"
        }
      ]
    },
    "Blockquote": {
      "body": "{{< blockquote type=\"[[type]]\" text=\"[[&selection]]\" >}}",
      "description": "Creates a blockquote",
      "fields": [
        {
          "name": "type",
          "title": "Type",
          "type": "choice",
          "choices": [
            "info",
            "important"
          ],
          "default": "info"
        },
        {
          "name": "selection",
          "title": "Selection",
          "type": "string",
          "default": "FM_SELECTED_TEXT"
        }
      ]
    }
  }
